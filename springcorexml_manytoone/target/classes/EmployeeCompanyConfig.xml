<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean class ="com.jspiders.springcorexml_manytoone.beans.EmployeeBean" name = "employee1">
	    <property name="id" value ="1"/>
	    <property name="name" value ="Abhishek"/>
	    <property name="salary" value ="10000"/>
	    <property name="company" ref="company1"/>	
	</bean>
	
	<bean class ="com.jspiders.springcorexml_manytoone.beans.EmployeeBean" name = "employee2">
	    <property name="id" value ="2"/>
	    <property name="name" value ="Abhi"/>
	    <property name="salary" value ="40000"/>
	    <property name="company" ref="company1"/>	
	</bean>
	
	<bean class ="com.jspiders.springcorexml_manytoone.beans.CompanyBean" name = "company1">
	    <property name="id" value ="1"/>
	    <property name="name" value ="wipro"/>
	    <property name="city" value ="pune"/>	
	</bean>
	
	

</beans>