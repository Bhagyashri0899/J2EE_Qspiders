<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    
    <bean class="com.jspiders.springcorexml_manytomany.beans.MovieBean" name="movie1">
		<property name="id" value="1"/>
		<property name="name" value="Drishyam"/>
		<property name="users">
			<list>
				<bean class="com.jspiders.springcorexml_manytomany.beans.UserBean" name="user1">
					<property name="id" value="1" />
					<property name="name" value="kashi" />
					<property name="email" value="kashi@gmail.com" />
				</bean>

				<bean class="com.jspiders.springcorexml_manytomany.beans.UserBean" name="user2">
					<property name="id" value="2" />
					<property name="name" value="Krishna" />
					<property name="email" value="krishna@gmail.com" />
				</bean>
			</list>
		</property>
	</bean>

	<bean class="com.jspiders.springcorexml_manytomany.beans.MovieBean" name="movie2">
		<property name="id" value="2"/>
		<property name="name" value="Ved"/>
		<property name="users">
			<list>
				<bean class="com.jspiders.springcorexml_manytomany.beans.UserBean" name="user1">
					<property name="id" value="01" />
					<property name="name" value="sakshi" />
					<property name="email" value="sashi@gmail.com" />
				</bean>
				
				<bean class="com.jspiders.springcorexml_manytomany.beans.UserBean" name="user2">
					<property name="id" value="02" />
					<property name="name" value="Kiran" />
					<property name="email" value="kiran@gmail.com" />
				</bean>
			</list>
		</property>
	</bean>
</beans>